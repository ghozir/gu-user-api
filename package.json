{
  "name": "core-edu-pijar-auth-api",
  "version": "1.0.0",
  "description": "Authentication service for Pijar Skeolah Online",
  "main": "index.js",
  "engines": {
    "node": "12.14.0"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "junit": "_mocha \"test/**/*_test.js\" --timeout=5000 --reporter mocha-junit-reporter",
    "test": "mocha \"test/**/*_test.js\" --timeout=5000 --exit",
    "cover": "nyc _mocha \"test/**/*_test.js\" --recursive --exit",
    "slint": "sonarlint --src 'bin/**' --tests 'test/**'",
    "lint": "eslint bin test",
    "lint:fix": "npm run lint -- --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.playcourt.id/telkomdev-ian/sample-backend-service"
  },
  "keywords": [
    "codebase",
    "api",
    "backend",
    "nodejs",
    "telkom"
  ],
  "author": "Telkom Indonesia",
  "license": "(ISC OR GPL-3.0)",
  "bugs": {
    "url": "https://gitlab.playcourt.id/telkomdev/api-mockup-management/issues"
  },
  "homepage": "https://gitlab.playcourt.id/telkomdev/api-mockup-management#readme",
  "dependencies": {
    "argon2": "^0.27.1",
    "aws-sdk": "^2.304.0",
    "axios": "^0.21.0",
    "azure-storage": "^2.10.3",
    "confidence": "^4.0.2",
    "crypto-js": "^4.0.0",
    "cryptr": "^6.0.2",
    "dotenv": "^6.1.0",
    "elastic-apm-node": "^3.13.0",
    "elasticsearch": "^15.4.1",
    "events": "1.1.1",
    "joi": "^17.1.1",
    "jsftp": "2.0.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.20",
    "minio": "^7.0.16",
    "mongodb": "^3.6.3",
    "morgan": "^1.10.0",
    "mysql": "^2.15.0",
    "nodemon": "^2.0.3",
    "passport": "0.4.0",
    "passport-http": "0.3.0",
    "pg": "^7.11.0",
    "query-string": "5.0.1",
    "randomstring": "^1.1.5",
    "raven": "2.2.1",
    "redis": "^3.0.2",
    "request-promise": "^4.2.6",
    "restify": "8.3.3",
    "restify-cors-middleware": "1.1.1",
    "restify-errors": "8.0.0",
    "rewire": "^5.0.0",
    "serve-static-restify": "3.0.0",
    "util": "0.10.3",
    "uuid": "3.1.0",
    "validate.js": "0.12.0",
    "winston": "^3.3.3",
    "winston-logstash": "^0.4.0",
    "winston-logstash-transport": "^2.0.0",
    "winston3-logstash-transport": "^1.2.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "hippie": "^0.6.1",
    "mocha": "^7.1.2",
    "mocha-junit-reporter": "^1.17.0",
    "nyc": "^15.0.1",
    "pre-commit": "^1.2.2",
    "sinon": "^4.2.2",
    "sonarlint": "^1.1.0"
  },
  "nyc": {
    "lines": 10,
    "statements": 10,
    "functions": 10,
    "branches": 10,
    "check-coverage": true,
    "include": [
      "bin/**/*.js"
    ],
    "exclude": [
      "bin/app/**/*.js"
    ],
    "reporter": [
      "html",
      "text",
      "lcov",
      "text-summary"
    ],
    "all": true,
    "cache": true,
    "report-dir": "./coverage"
  },
  "pre-commit": [
    "lint"
  ]
}
